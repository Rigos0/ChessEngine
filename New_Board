class Board(object):
    def __init__(self, squares, positions):
        self.squares = squares
        self.positions = positions

    def occupation(self):
        occupation = []
        square_counter = 27
        while square_counter < 119:
            if square_counter in self.positions:
                occupation.append(square_counter)
            square_counter += 1
        return occupation


