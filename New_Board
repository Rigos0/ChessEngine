w_bishops = [29, 32]
w_rooks = [27, 34]
w_knights = [28, 33]
w_king = [31]
w_queen = [30]
w_pawns = [39, 40, 41, 42, 43, 44, 45, 46]
b_bishops = [113, 116]
b_rooks = [111, 118]
b_knights = [112, 117]
b_king = [115]
b_queen = [114]
b_pawns = [99, 100, 101, 102, 103, 104, 105, 106]

w_positions = w_bishops + w_king + w_knights + w_pawns + w_queen + w_rooks
b_positions = b_bishops + b_king + b_knights + b_pawns + b_queen + b_rooks

board = Board(all_squares, w_positions)
occ = board.occupation()
print(occ)

board = Board(all_squares, b_positions)
occ = board.occupation()
print(occ)





